{
  "name": "react-net-kit",
  "version": "1.0.3",
  "scripts": {
    "dev": "npmize dev",
    "build": "npmize build",
    "vite": "vite"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.cts"
    },
    "./ws": {
      "require": "./dist/ws.cjs",
      "import": "./dist/ws.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "ws": [
        "./dist/ws.d.cts"
      ]
    }
  },
  "peerDependencies": {
    "axios": "*",
    "react": "*",
    "socket.io-client": "*"
  },
  "devDependencies": {
    "@types/node": "^22.7.7",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "axios": "*",
    "npmize": "^1.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "*",
    "typescript": "^5.2.2",
    "vite": "^5.4.9"
  }
}
